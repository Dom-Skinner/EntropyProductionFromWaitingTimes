     "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["\[ScriptCapitalL]", "*"], "p"}], " ", "+", " ", 
       RowBox[{
        FractionBox["l", "2"], "a"}]}], " ", "=", " ", "0"}]}]}], 
   TraditionalForm]],ExpressionUUID->"0ffc087f-4562-433e-a8cf-2b0da7b14fd9"]
