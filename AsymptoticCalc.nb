     RowBox[{
      SubscriptBox["\[ScriptCapitalL]T", "2"], " ", "=", " ", 
      RowBox[{
       RowBox[{"-", "2"}], " ", 
       SubscriptBox["T", "1"]}]}], "\[IndentingNewLine]", "\t", 
     RowBox[{
      SubscriptBox["t", "2"], " ", "=", " ", 
      RowBox[{
       FractionBox[
        SuperscriptBox["l", "2"], "F"], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["a", "0"], " ", 
          RowBox[{
           SubscriptBox["T", "2"], "(", 
           SubscriptBox["x", "0"], ")"}]}], " ", "+", " ", 
         RowBox[{
          SubscriptBox["a", "1"], " ", 
          RowBox[{
           SubscriptBox["T", "2"], "(", 
