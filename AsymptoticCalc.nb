Cell[TextData[{
 "We take ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", " ", "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox["a", "0"], 
      RowBox[{"\[Delta]", "(", 
       RowBox[{"x", "-", 
        SubscriptBox["x", "0"]}], ")"}]}], "+", 
     RowBox[{
      SubscriptBox["a", "1"], 
      RowBox[{"\[Delta]", "(", 
       RowBox[{"x", "-", 
        SubscriptBox["x", "1"]}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"074fac4f-bd43-4fe8-8028-56d3092ea7f3"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"b", " ", "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox["b", "0"], 
      RowBox[{"\[Delta]", "(", 
       RowBox[{"x", "-", 
        SubscriptBox["x", "0"]}], ")"}]}], "+", 
     RowBox[{
      SubscriptBox["b", "1"], 
      RowBox[{"\[Delta]", "(", 
       RowBox[{"x", "-", 
        SubscriptBox["x", "1"]}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"c7f50803-4dd9-4ff0-a3f8-cfb38d33af2b"],
 ", so that \nmin ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Sigma]", " ", "=", 
    RowBox[{
     FractionBox[
      SuperscriptBox["F", "2"], 
      RowBox[{"2", "D"}]], " ", "+", " ", 
     RowBox[{
      RowBox[{"p", "(", 
       SubscriptBox["x", "0"], ")"}], " ", 
      SubscriptBox["b", "0"], 
      RowBox[{"ln", "(", 
       RowBox[{
        SubscriptBox["b", "0"], "/", 
        SubscriptBox["a", "0"]}], ")"}]}], " ", "+", 
     RowBox[{
      RowBox[{"p", "(", 
       SubscriptBox["x", "1"], ")"}], " ", 
      SubscriptBox["b", "1"], 
      RowBox[{"ln", "(", 
       RowBox[{
        SubscriptBox["b", "1"], "/", 
        SubscriptBox["a", "1"]}], ")"}]}], " ", "+", 
     RowBox[{
      FractionBox["1", "2"], 
      SubscriptBox["a", "0"], 
      RowBox[{"ln", "(", 
       RowBox[{
        SubscriptBox["a", "0"], "/", 
        SubscriptBox["b", "0"]}], ")"}]}], " ", "+", 
     RowBox[{
      FractionBox["1", "2"], 
      SubscriptBox["a", "1"], 
      RowBox[{"ln", "(", 
       RowBox[{
        SubscriptBox["a", "1"], "/", 
        SubscriptBox["b", "1"]}], ")"}], " "}]}]}], TraditionalForm]],
  ExpressionUUID->"47fb3e59-bd08-47f2-8aa2-752e24e9b412"],
 "\n\tsubject to the constraints that\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Integral]", 
       RowBox[{"dx", " ", "p"}]}], " ", "=", 
      FractionBox["1", "2"]}], "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{
       SubscriptBox["a", "0"], "+", 
       SubscriptBox["a", "1"]}], " ", "=", "1"}], "\[IndentingNewLine]", "\t", 
     RowBox[{"\[ScriptCapitalL]", " ", "=", " ", 
      RowBox[{"F", " ", 
       RowBox[{
        SubscriptBox["\[PartialD]", "x"], 
        RowBox[{"+", " ", "D"}]}], " ", 
       RowBox[{
        SubscriptBox["\[PartialD]", "xx"], " ", 
        RowBox[{"-", " ", "b"}]}]}]}], "\[IndentingNewLine]", "\t", 
     RowBox[{
      SuperscriptBox["\[ScriptCapitalL]", "*"], " ", "=", " ", 
      RowBox[{
       RowBox[{"-", "F"}], " ", 
       RowBox[{
        SubscriptBox["\[PartialD]", "x"], 
        RowBox[{"+", " ", "D"}]}], " ", 
       RowBox[{
        SubscriptBox["\[PartialD]", "xx"], " ", 
        RowBox[{"-", " ", "b"}]}]}]}], " ", "\[IndentingNewLine]", "\t", 
     RowBox[{
      SubscriptBox["\[ScriptCapitalL]T", "1"], "=", " ", 
      RowBox[{"-", "1"}]}], "\[IndentingNewLine]", "\t", 
     RowBox[{
      SubscriptBox["\[ScriptCapitalL]T", "2"], " ", "=", " ", 
      RowBox[{
       RowBox[{"-", "2"}], " ", 
       SubscriptBox["T", "1"]}]}], "\[IndentingNewLine]", "\t", 
     RowBox[{
      SubscriptBox["t", "2"], " ", "=", " ", 
      RowBox[{
       RowBox[{
        SubscriptBox["a", "0"], " ", 
        RowBox[{
         SubscriptBox["T", "2"], "(", 
         SubscriptBox["x", "0"], ")"}]}], " ", "+", " ", 
       RowBox[{
        SubscriptBox["a", "1"], " ", 
        RowBox[{
         SubscriptBox["T", "2"], "(", 
         SubscriptBox["x", "1"], ")"}]}]}]}], "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["\[ScriptCapitalL]", "*"], "p"}], " ", "+", " ", 
       RowBox[{
        FractionBox["1", "2"], "a"}]}], " ", "=", " ", "0"}]}]}], 
   TraditionalForm]],ExpressionUUID->"7672605a-ced5-417d-8ed5-052fc95aa257"]
}], "Text",
