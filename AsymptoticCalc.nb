      SuperscriptBox["D", "2"]}]}]}], TraditionalForm]],ExpressionUUID->
  "72cc6a3e-07f4-4896-a1b3-d577f2e75c48"],
 ", the problem becomes\nmin ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Sigma]", " ", "=", 
    RowBox[{"F", "(", 
     RowBox[{
      FractionBox["l", "2"], " ", "+", " ", 
      RowBox[{
       RowBox[{"p", "(", 
        SubscriptBox["x", "0"], ")"}], " ", 
       SubscriptBox["b", "0"], 
       RowBox[{"ln", "(", 
        RowBox[{
         SubscriptBox["b", "0"], "/", 
         SubscriptBox["a", "0"]}], ")"}]}], " ", "+", 
      RowBox[{
       RowBox[{"p", "(", 
        SubscriptBox["x", "1"], ")"}], " ", 
       SubscriptBox["b", "1"], 
       RowBox[{"ln", "(", 
        RowBox[{
         SubscriptBox["b", "1"], "/", 
         SubscriptBox["a", "1"]}], ")"}]}], " ", "+", 
      RowBox[{
       FractionBox["1", "2"], 
       SubscriptBox["a", "0"], 
       RowBox[{"ln", "(", 
        RowBox[{
         SubscriptBox["a", "0"], "/", 
         SubscriptBox["b", "0"]}], ")"}]}], " ", "+", 
      RowBox[{
       FractionBox["1", "2"], 
       SubscriptBox["a", "1"], 
       RowBox[{"ln", "(", 
        RowBox[{
         SubscriptBox["a", "1"], "/", 
         SubscriptBox["b", "1"]}], ")"}]}]}], " ", ")"}]}], TraditionalForm]],
  ExpressionUUID->"67f9d963-8908-4d7f-bab1-064d6ccecfa9"],
 "\n\tsubject to the constraints that\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Integral]", 
       RowBox[{"dx", " ", "p"}]}], " ", "=", 
      FractionBox["1", "2"]}], "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{
       SubscriptBox["a", "0"], "+", 
       SubscriptBox["a", "1"]}], " ", "=", 
      RowBox[{"1", "/", "F"}]}], "\[IndentingNewLine]", "\t", 
     RowBox[{"\[ScriptCapitalL]", " ", "=", " ", 
      RowBox[{"l", " ", 
       RowBox[{
        SubscriptBox["\[PartialD]", "x"], 
        RowBox[{"+", "  ", 
         RowBox[{
          SubscriptBox["\[PartialD]", "xx"], " ", 
          RowBox[{"-", " ", "lb"}]}]}]}]}]}], "\[IndentingNewLine]", "\t", 
     RowBox[{
      SuperscriptBox["\[ScriptCapitalL]", "*"], " ", "=", " ", 
