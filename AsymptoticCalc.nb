Cell[TextData[{
 "We note that the combination F/D appears often so if possible we would wish \
to replace ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"F", "/", "D"}], " ", "=", " ", "l"}], TraditionalForm]],
  ExpressionUUID->"5c50881a-313e-4152-8eba-7931028e162b"],
 ". Replacing ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "i"], " ", "\[Rule]", 
    RowBox[{"F", " ", 
     SubscriptBox["a", "i"]}]}], TraditionalForm]],ExpressionUUID->
  "048629ce-7b4e-4009-a00c-1ab20a95924e"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["b", "i"], "\[Rule]", 
    SubscriptBox["Fb", "i"]}], TraditionalForm]],ExpressionUUID->
  "21ed5003-9cfb-4227-a7f6-854e2e4a2ac9"],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["T", 
      RowBox[{"1", " "}]], "\[Rule]", " ", 
     RowBox[{
      SubscriptBox["T", "1"], "/", "D"}]}], ",", " ", 
    RowBox[{
     SubscriptBox["T", 
      RowBox[{"2", " "}]], "\[Rule]", " ", 
     RowBox[{
